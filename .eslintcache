rent.select();\n    copyRef.current.setSelectionRange(0, 99999);\n    document.execCommand(\"copy\");\n  };\n\n  return (\n    <>\n      {!isHidden && post && (\n        <>\n          <Container>\n            <Vote>\n              <VoteButton\n                type=\"button\"\n                isUpvoted={vote === 1}\n                onClick={() => {\n                  // eslint-disable-next-line no-unused-expressions\n                  currentUser\n                    ? handleUpvote(\"posts\", postId, currentUser.uid, vote)\n                    : setIsEntryOpen(true);\n                }}\n              >\n                <IconUp />\n              </VoteButton>\n              <span>{votes}</span>\n              <VoteButton\n                type=\"button\"\n                isDownvoted={vote === -1}\n                onClick={() => {\n                  // eslint-disable-next-line no-unused-expressions\n                  currentUser\n                    ? handleDownvote(\"posts\", postId, currentUser.uid, vote)\n                    : setIsEntryOpen(true);\n                }}\n              >\n                <IconDown />\n              </VoteButton>\n            </Vote>\n            <Main to={`/b/${post.subreadit.name}`}>\n              <Link to={`/b/${post.subreadit.name}`}>\n                <Informations>\n                  <Link to={`/b/${post.subreadit.name}`}>\n                    <BoldPrimary>\n                      b/\n                      {post.subreadit.name}\n                    </BoldPrimary>\n                  </Link>\n                  <span> • Posted by </span>\n                  <Link to={`/u/${post.author.id}`}>\n                    u/\n                    {post.author.name}\n                    &nbsp;\n                  </Link>\n                  <Link to={`/b/${post.subreadit.name}/${postId}`}>\n                    {formatDistanceStrict(\n                      new Date(post.date.seconds * 1000),\n                      new Date()\n                    )}\n                    &nbsp; ago\n                  </Link>\n                  {subscriptions.filter(\n                    (subreadit) => subreadit.id === post.subreadit.id\n                  ).length === 0 && (\n                    <JoinButton\n                      onClick={(e) => {\n                        e.preventDefault();\n                        joinSubreadit(currentUser.uid, post.subreadit);\n                      }}\n                    >\n                      Join\n                    </JoinButton>\n                  )}\n                </Informations>\n                {post.type !== \"link\" && (\n                  <Link to={`/b/${post.subreadit.name}/${postId}`}>\n                    <Title>{post.title}</Title>\n                  </Link>\n                )}\n                <>\n                  {post.type === \"post\" &&\n                    post.content &&\n                    renderText(post.content, post.subreadit.name, postId)}\n                  {post.type === \"image\" &&\n                    renderImages(post.content, post.title)}\n                  {post.type === \"link\" && renderLink(post.content, post.title)}\n                </>\n              </Link>\n              <Buttons>\n                <ButtonLink to={`/b/${post.subreadit.name}/${postId}`}>\n                  <IconComment />\n                  {commentsNumber} Comment\n                  {commentsNumber !== 1 && \"s\"}\n                </ButtonLink>\n                <Button\n                  type=\"button\"\n                  onClick={(e) => {\n                    e.preventDefault();\n                    handleSave(currentUser.uid, postId, \"post\");\n                  }}\n                >\n                  {saved.includes(postId) ? <IconSaved /> : <IconSave />}\n                  Save\n                </Button>\n                <Button\n                  type=\"button\"\n                  onClick={(e) => {\n                    e.preventDefault();\n                    setIsHidden(true);\n                  }}\n                >\n                  <IconHide />\n                  Hide\n                </Button>\n                <Button\n                  type=\"button\"\n                  onClick={(e) => {\n                    e.preventDefault();\n                    copyLink();\n                  }}\n                >\n                  <IconLink />\n                  Share\n                  <InputCopy\n                    type=\"text\"\n                    value={`https://breadit-296d8.web.app/b/${post.subreadit.name}/${postId}`}\n                    ref={copyRef}\n                    readOnly\n                  />\n                </Button>\n              </Buttons>\n            </Main>\n          </Container>\n\n          <EntryModal\n            isOpen={isEntryOpen}\n            onRequestClose={() => setIsEntryOpen(false)}\n          >\n            <Entry close={() => setIsEntryOpen(false)} />\n          </EntryModal>\n        </>\n      )}\n    </>\n  );\n}\n\nPostPreview.propTypes = {\n  postId: PropTypes.string.isRequired,\n};\nexport default PostPreview;\n\nconst colors = {\n  primary: \"black\",\n  secondary: \"grey\",\n  background: \"rgb(255, 255, 255)\",\n  voteBackground: \"rgb(247, 244, 240)\",\n  arrowBackground: \"rgb(237, 212, 194)\",\n  upvote: \"rgb(179, 72, 54)\",\n  downvote: \"rgb(70, 153, 147)\",\n  neutral: \"rgb(209, 163, 155)\",\n};\n\nconst Container = styled.article`\n  border: 1px solid ${colors.neutral};\n  display: flex;\n  border-radius: 0.25rem;\n  background: ${colors.background};\n\n  &:hover {\n    border: 1px solid ${colors.upvote};\n  }\n`;\n\nconst BoldPrimary = styled.div`\n  font-weight: 600;\n  color: ${colors.primary};\n`;\n\nconst Informations = styled.div`\n  display: flex;\n  font-size: 0.75rem;\n  color: ${colors.secondary};\n  padding: 0.5rem;\n\n  & > * {\n    margin-right: 0.25rem;\n  }\n\n  & > a:hover {\n    text-decoration: underline;\n  }\n`;\n\nconst Vote = styled.div`\n  display: flex;\n  