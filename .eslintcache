ername,\n    setUsername,\n    password,\n    setPassword,\n    emailError,\n    passwordError,\n    usernameError,\n    loading,\n    checkUsername,\n    checkEmail,\n    checkPassword,\n    handleSignUp,\n  };\n}\n\nexport default useSignUp;\n","/home/aureen/the_odin_project/breadit/src/hooks/useUser.js",[],"/home/aureen/the_odin_project/breadit/src/hooks/useSignIn.js",[],"/home/aureen/the_odin_project/breadit/src/components/entry/Entry.jsx",[],"/home/aureen/the_odin_project/breadit/src/components/entry/ForgotPassword.jsx",[],"/home/aureen/the_odin_project/breadit/src/hooks/useForgotPassword.js",[],{"ruleId":"142","severity":2,"message":"143","line":3,"column":17,"nodeType":"144","endLine":3,"endColumn":28},{"ruleId":"145","replacedBy":"146"},{"ruleId":"147","replacedBy":"148"},{"ruleId":"149","replacedBy":"150"},{"ruleId":"151","replacedBy":"152"},{"ruleId":"153","replacedBy":"154"},{"ruleId":"155","severity":2,"message":"156","line":1,"column":19,"nodeType":null,"endLine":1,"endColumn":26,"fix":"157"},{"ruleId":"155","severity":2,"message":"158","line":4,"column":10,"nodeType":null,"endLine":8,"endColumn":4,"fix":"159"},{"ruleId":"160","severity":2,"message":"161","line":5,"column":5,"nodeType":"162","endLine":5,"endColumn":10,"fix":"163"},{"ruleId":"155","severity":2,"message":"164","line":11,"column":20,"nodeType":null,"endLine":11,"endColumn":20,"fix":"165"},{"ruleId":"155","severity":2,"message":"156","line":1,"column":19,"nodeType":null,"endLine":1,"endColumn":26,"fix":"166"},{"ruleId":"155","severity":2,"message":"158","line":4,"column":10,"nodeType":null,"endLine":8,"endColumn":4,"fix":"167"},{"ruleId":"160","severity":2,"message":"161","line":5,"column":5,"nodeType":"162","endLine":5,"endColumn":10,"fix":"168"},{"ruleId":"155","severity":2,"message":"164","line":11,"column":20,"nodeType":null,"endLine":11,"endColumn":20,"fix":"169"},{"ruleId":"155","severity":2,"message":"156","line":1,"column":19,"nodeType":null,"endLine":1,"endColumn":26,"fix":"170"},{