ot.data());\n    });\n    return unsubscribe;\n  }, []);\n\n  const handleEdit = () => {\n    if (!edit) return;\n    editComment(commentId, edit);\n    setIsEditing(false);\n  };\n\n  return (\n    <>\n      {comment && (\n        <>\n          <Container className={className}>\n            <>\n              {comment.isDeleted ? (\n                <Information\n                  author={comment.author}\n                  date={comment.date}\n                  isDeleted\n                />\n              ) : (\n                <Information author={comment.author} date={comment.date} />\n              )}\n            </>\n\n            {isEditing ? (\n              <Form onSubmit={handleEdit}>\n                <TextEditor\n                  type=\"comment\"\n                  sendContent={setEdit}\n                  prevContent={comment.content}\n                  placeholder=\"What are your thoughts?\"\n                />\n\n                <ButtonsForm>\n                  <ButtonFilled type=\"submit\">Edit</ButtonFilled>\n                  <Button type=\"button\" onClick={() => setIsEditing(false)}>\n                    Cancel\n                  </Button>\n                </ButtonsForm>\n              </Form>\n            ) : (\n              <>\n                <>\n                  {comment.isDeleted ? (\n                    <p>[deleted]</p>\n                  ) : (\n                    <div>\n                      {comment.content &&\n                        redraft(JSON.parse(comment.content), renderers)}\n                    </div>\n                  )}\n                </>\n                <ButtonsGroup>\n                  <Vote type=\"comments\" docId={commentId} user={currentUser} />\n                  <Buttons\n                    user={currentUser}\n                    commentId={commentId}\n                    postId={comment.post.id}\n                    subreadit={comment.post.subreadit.name}\n                    onReply={() => setIsReplying(!isReplying)}\n                  />\n                  {!comment.isDeleted && (\n                    <ExtraButtons\n                      // Only the author can edit\n                      canEdit={\n                        currentUser && currentUser.uid === comment.author.id\n                      }\n                      // Author and subreadits mods can delete\n                      canDelete={\n                        !comment.isDeleted &&\n                        currentUser &&\n                        (currentUser.uid === comment.author.id ||\n                          permissions[currentUser.uid])\n                      }\n                      onEdit={() => {\n                        setIsEditing(true);\n                        setEdit(comment.content);\n                      }}\n                      onDelete={() => deleteComment(commentId)}\n                      copy={`${comment.post.subreadit.name}/${comment.post.id}/${commentId}`}\n                      type=\"comment\"\n                    />\n                  )}\n                </ButtonsGroup>\n\n                {isReplying && (\n                  <Form\n                    onSubmit={(e) => {\n                      e.preventDefault();\n                      if (!reply) return;\n                      createComment(\n                        comment.post,\n                        currentUser,\n                        reply,\n                        commentId\n                      );\n                      setIsReplying(false);\n                    }}\n                  >\n                    <TextEditor type=\"comment\" sendContent={setReply} />\n                    <ButtonsForm>\n                      <ButtonFilled type=\"submit\">Comment</ButtonFilled>\n                      <Button\n                        type=\"button\"\n                        onClick={() => setIsReplying(false)}\n                      >\n                        Cancel\n                      </Button>\n                    </ButtonsForm>\n